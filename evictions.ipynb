import requests
import pandas as pd

# Use the Socrata API v3 query.json endpoint
url = "https://data.sfgov.org/api/v3/views/5cei-gny5/query.json"

# Try to fetch it
resp = requests.get(url)
print("status:", resp.status_code)

# Naively turn into Python object
data = None
try:
    data = resp.json()
except Exception as e:
    print("json parse failed", e)

# Dump what we got
print(data)

# Now try crude conversion into a DataFrame â€” this will likely be wrong
df = pd.DataFrame(data)
print(df.head())
print("shape:", df.shape)
